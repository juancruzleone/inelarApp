{
  "expo": {
    "name": "inelarapp",
    "slug": "inelarapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconoApp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/iconoApp.png",
      "resizeMode": "contain",
      "backgroundColor": "#222222"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juanleone.inelarapp",
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["http", "https"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconoApp.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/iconoApp.png",
      "package": "com.juanleone.inelarapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "*"
            },
            {
              "scheme": "http",
              "host": "*"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "permissions": ["INTERNET", "CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5a659921-5e16-4cca-905b-a9c126f6f463"
      }
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir a $(PRODUCT_NAME) acceder a tu c√°mara."
        }
      ]
    ]
  }
}

